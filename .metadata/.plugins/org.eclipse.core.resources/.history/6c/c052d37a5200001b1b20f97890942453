package board;

public class Pawn extends Piece{

	public Pawn(Board givenBoard) {
		super(givenBoard);
		// TODO Auto-generated constructor stub
	}

	@Override
	public PieceType getType() {
		// TODO Auto-generated method stub
		return PieceType.PAWN;
	}

	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return new String("P");
	}

}
